config AdGuardHome 'AdGuardHome'
	option enabled '0'
	option httpport '3000'
	option redirect 'none'
	option configpath '/etc/config/AdGuardHome.yaml'
	option workdir '/usr/bin/AdGuardHome_data/'
	option logfile '/usr/bin/AdGuardHome_data/AdGuardHome.log'
	option verbose '0'
	option binpath '/usr/bin/AdGuardHome'
	option upxflag ''

