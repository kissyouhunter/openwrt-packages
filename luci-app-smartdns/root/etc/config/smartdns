
config smartdns
	option port '6053'
	option tcp_server '1'
	option rr_ttl_min '300'
	option seconddns_tcp_server '1'
	option seconddns_no_speed_check '0'
	option seconddns_no_rule_addr '0'
	option seconddns_no_rule_nameserver '0'
	option seconddns_no_rule_ipset '0'
	option seconddns_no_cache '0'
	option coredump '0'
	option seconddns_no_dualstack_selection '1'
	option seconddns_server_group 'overseas'
	option seconddns_no_rule_soa '0'
	option force_aaaa_soa '1'
	option rr_ttl_max '3600'
	option seconddns_port '7913'
	option server_name 'SmartDNS'
	option enabled '1'
	option rr_ttl '5'
	option dualstack_ip_selection '1'
	option redirect 'dnsmasq-upstream'
	option ipv6_server '1'
	option prefetch_domain '1'
	option serve_expired '1'
	list old_redirect 'dnsmasq-upstream'
	list old_port '6053'
	list old_enabled '1'

config server
	option enabled '1'
	option name 'Google'
	option ip '8.8.8.8'
	option port '853'
	option type 'tls'
	option server_group 'overseas'
	option blacklist_ip '0'

